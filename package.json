{
  "name": "minecraft-splash-ts",
  "version": "1.0.1",
  "description": "An updated collection of minecraft's title screen messages (java + bedrock).",
  "author": "cuppachino <cuppachino.dev@gmail.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cuppachino/minecraft-splash-ts.git"
  },
  "homepage": "https://github.com/Cuppachino/minecraft-splash-ts#readme",
  "bugs": {
    "url": "https://github.com/Cuppachino/minecraft-splash-ts/issues"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "minecraft",
    "title",
    "splash",
    "text",
    "list",
    "typescript",
    "bedrock edition",
    "java edition"
  ],
  "main": "dist/index.mjs",
  "type": "module",
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node-esm ./src/index.mts",
    "build:swc": "ts-node ./src/index.mts",
    "build:tsc": "tsc --build --clean && tsc",
    "build:clean": "tsc --build --clean",
    "start": "node ./dist/index.mjs"
  },
  "devDependencies": {
    "@cuppachino/type-space": "^1.10.1",
    "@swc/core": "^1.3.27",
    "@swc/helpers": "^0.4.14",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "colorix": "^1.0.3"
  }
}
